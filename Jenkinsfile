pipeline {
  agent any
  stages {
    stage('build') {
      parallel {
        stage('build') {
          steps {
            sh 'echo \'build\''
          }
        }
        stage('oiajsdadasda') {
          steps {
            sh 'echo \'jelo\''
          }
        }
        stage('erroradasdad') {
          steps {
            sh 'echdo \'jel . jnknk knknko\''
          }
        }
      }
    }
    stage('dadsasda') {
      steps {
        sh 'echo \'jelasdmasmdlmasdlasdo\''
      }
    }
    stage('asdadasda') {
      parallel {
        stage('asdadasda') {
          steps {
            sh 'echo \'jedaasdasda adasd ad adsalo\''
          }
        }
        stage('oasdjaodja') {
          steps {
            sh 'echo \'jesssslo\''
          }
        }
      }
    }
    stage('lamper') {
      steps {
        sh 'echo \'oasjdasd adsas\''
      }
    }
  }
}